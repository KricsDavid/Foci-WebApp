@page
@model Orai.Pages.Shared.TabellaModel
@{
    ViewData["Title"] = "Tabella";

}
<h1>Tabella</h1>
<table class="table"> 
    <thead></thead>
    <tbody>
        <tr>
            <th>Helyezés</th>
            <th>Csapat neve</th>
            <th>Játszott mérkőzés</th>
            <th></th>

            
        </tr>
        @for(int i =0; i<Model.meccsek.Count(); i++)
        {
            
            <tr>
                <td>@(i+1)</td>
                <td>@Model.meccsek[i].HazaiCsapat</td>0
            </tr>
        }

    </tbody>
    
</table>